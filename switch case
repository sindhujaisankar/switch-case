# switch-case
program using switch case
#include<stdio.h>
int main()
{
  int x;
  printf("enter a number: ");
  scanf("%d",&x);
  switch(x)
  {
    case 1:
      printf("pizza\n,239");
      break;
    case 2:
      printf("burger\n129");
      break;
    case 3:
      printf("pasta\n179");
      break;
    case 4:
      printf("french fries\n99");
      break;
    case 5:
      printf("sandwich\n149");
      break;
    }
}
